<html>
	<head>
		<!-- CSS only-->
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
		<!-- JavaScript Bundle with Popper -->
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
		
		<script>
			$(document).ready(function(){
				
				$('#btn').click(function(){
					let fileInfo = $('#fileSelector')[0].files[0];
					//console.log(fileInfo);
					let render = new FileReader();
					render.readAsDataURL(fileInfo);//주소를 받는거
					
					let imgString='';
					render.onloadend = function(){
						imgString = render.result;
						console.log(imgString); //이미지를 받아올 녀석의 주소를 받아서, 그결과를 해석해서 이미지스트링에 넣는것.
						$.ajax({
						'url':'/interfacePhotoSave',
						'type':'post',
						'datatype':'json',
						'data':{'img':imgString},
						'success':function(results){
							console.log('succeed')
						}
					
					});
					}
					
				});
			});
		
		</script>
	</head>
	<body>
		<form>
			<input id='fileSelector' type='file'>
			<input id='btn' type='button' value='ok'>
		</form>
		
	</body>
	
</html>